{
  "hosting": {
    "public": "public",
    "redirects": [
      {
        "source": "/:version/docs",
        "destination": "/:version/docs/index.html",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/**",
        "function": "sendStoredFile"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
