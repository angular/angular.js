{
  "hosting": {
    "public": "../../uploadDocs",
    "ignore": [
      "/index.html",
      "/index-debug.html",
      "/index-jquery.html"
    ],
    "redirects": [
      {
        "source": "/error/:namespace\\::error*",
        "destination": "/error/:namespace/:error*",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index-production.html"
      },
      {
        "source": "/index.html",
        "destination": "/index-production.html"
      },
      {
        "source": "**/*!(.jpg|.jpeg|.gif|.png|.html|.js|.map|.json|.css|.svg|.ttf|.txt|.woff|.woff2|.eot)",
        "destination": "/index-production.html"
      }
    ]
  }
}